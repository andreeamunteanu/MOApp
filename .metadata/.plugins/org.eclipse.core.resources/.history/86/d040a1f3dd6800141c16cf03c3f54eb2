package storage;

import com.example.moapplic.User;

public class Database {

	private User[] users;

	public Database() {
		users = new User[100];
	}

	public void add(User user) {
		users[user.getId()] = user;// the position of the user will be it's
									// unique id so that it will be easy to find
	}

	public User[] getUsers() {
		return users;
	}

}
